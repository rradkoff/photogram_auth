<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
      <% @my_likes.each do |my_like| %>

        <td><%= my_like.user_id %></td>
        <td><%= my_like.photo.user_id %></td>
        <td><%= my_like.photo.image %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

